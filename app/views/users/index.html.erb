<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
	<table>
		<tr>
			<th>Name</th>
			<th>Email</th>
			<th></th>
		</tr>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.first_name + ' ' + user.last_name %></td>
				<td><%= user.email %></td>
				<td><%= link_to "Profile", users_show_path(:id => user.id) %></td>
			</tr>
		<% end %>
	</table>